-- ThÃªm má»¥c Fix FPS Ä‘á»ƒ tá»‘i Æ°u Ä‘á»“ há»a game Blox Fruits
Tab:AddToggle({
    Name = "Fix FPS : Äang hoáº¡t Ä‘á»™ng ğŸŸ¢",
    Default = false, -- Máº·c Ä‘á»‹nh lÃ  táº¯t
    Callback = function(state)
        fixFPS = state
        if fixFPS then
            -- Tá»‘i Æ°u hÃ³a FPS báº±ng cÃ¡ch thay Ä‘á»•i Ä‘á»“ há»a
            -- XÃ³a mÃ u sáº¯c cá»§a báº£n Ä‘á»“
            game.Lighting.Ambient = Color3.fromRGB(50, 50, 50)  -- LÃ m tá»‘i ná»n
            game.Lighting.OutdoorAmbient = Color3.fromRGB(50, 50, 50)  -- LÃ m tá»‘i ná»n ngoÃ i
            game.Lighting.Brightness = 0  -- Táº¯t Ã¡nh sÃ¡ng máº¡nh
            game.Lighting.TimeOfDay = "00:00:00"  -- Táº¯t Ã¡nh sÃ¡ng trá»i, lÃ m tá»‘i hoÃ n toÃ n
            game.Lighting.FogColor = Color3.fromRGB(50, 50, 50)  -- ThÃªm mÃ u sÆ°Æ¡ng mÃ¹ Ä‘á»ƒ giáº£m chi tiáº¿t
            game.Lighting.FogEnd = 100  -- Giáº£m táº§m nhÃ¬n xa cá»§a sÆ°Æ¡ng mÃ¹

            -- Chá»‰nh sá»­a terrain Ä‘á»ƒ lÃ m tá»‘i vÃ  giáº£m chi tiáº¿t
            game.Workspace.Terrain:SetMaterialColor(Enum.Material.Grass, Color3.fromRGB(30, 30, 30))  -- Thay mÃ u cá»
            game.Workspace.Terrain:SetMaterialColor(Enum.Material.SmoothPlastic, Color3.fromRGB(30, 30, 30))  -- Thay mÃ u Ä‘áº¥t
            game.Workspace.Terrain:SetMaterialColor(Enum.Material.WoodPlanks, Color3.fromRGB(30, 30, 30))  -- Thay mÃ u gá»—

            -- XÃ³a cÃ¡c hiá»‡u á»©ng khÃ´ng cáº§n thiáº¿t
            for _, effect in pairs(game.Workspace:GetDescendants()) do
                if effect:IsA("ParticleEmitter") or effect:IsA("Beam") then
                    effect.Enabled = false  -- Táº¯t cÃ¡c hiá»‡u á»©ng particle
                end
            end

            -- KhÃ´ng thay Ä‘á»•i cÃ¡c thuá»™c tÃ­nh UI hay áº£nh hÆ°á»Ÿng Ä‘áº¿n cÃ¡c pháº§n tá»­ giao diá»‡n
            print("Äá»“ há»a Ä‘Ã£ Ä‘Æ°á»£c tá»‘i Æ°u Ä‘á»ƒ tÄƒng FPS mÃ  khÃ´ng áº£nh hÆ°á»Ÿng Ä‘áº¿n UI.")
        else
            -- KhÃ´i phá»¥c láº¡i tráº¡ng thÃ¡i Ä‘á»“ há»a ban Ä‘áº§u
            game.Lighting.Ambient = Color3.fromRGB(128, 128, 128)  -- KhÃ´i phá»¥c Ã¡nh sÃ¡ng ban Ä‘áº§u
            game.Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)  -- KhÃ´i phá»¥c Ã¡nh sÃ¡ng ngoÃ i trá»i
            game.Lighting.Brightness = 2  -- KhÃ´i phá»¥c Ä‘á»™ sÃ¡ng
            game.Lighting.TimeOfDay = "14:00:00"  -- Giá»›i háº¡n Ã¡nh sÃ¡ng trá»i vá» ban ngÃ y
            game.Lighting.FogColor = Color3.fromRGB(255, 255, 255)  -- KhÃ´i phá»¥c mÃ u sÆ°Æ¡ng mÃ¹ máº·c Ä‘á»‹nh
            game.Lighting.FogEnd = 1000  -- KhÃ´i phá»¥c táº§m nhÃ¬n sÆ°Æ¡ng mÃ¹

            -- KhÃ´i phá»¥c terrain
            game.Workspace.Terrain:SetMaterialColor(Enum.Material.Grass, Color3.fromRGB(0, 255, 0))  -- KhÃ´i phá»¥c mÃ u cá»
            game.Workspace.Terrain:SetMaterialColor(Enum.Material.SmoothPlastic, Color3.fromRGB(255, 255, 255))  -- KhÃ´i phá»¥c mÃ u Ä‘áº¥t
            game.Workspace.Terrain:SetMaterialColor(Enum.Material.WoodPlanks, Color3.fromRGB(153, 101, 21))  -- KhÃ´i phá»¥c mÃ u gá»—

            -- KhÃ´i phá»¥c cÃ¡c hiá»‡u á»©ng Ä‘Ã£ táº¯t
            for _, effect in pairs(game.Workspace:GetDescendants()) do
                if effect:IsA("ParticleEmitter") or effect:IsA("Beam") then
                    effect.Enabled = true  -- Báº­t láº¡i cÃ¡c hiá»‡u á»©ng particle
                end
            end

            print("Äá»“ há»a Ä‘Ã£ Ä‘Æ°á»£c khÃ´i phá»¥c láº¡i mÃ  khÃ´ng áº£nh hÆ°á»Ÿng Ä‘áº¿n UI.")
        end
    end
})

