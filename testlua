getgenv().Team = "Pirate"
-- Tá»± Ä‘á»™ng chá»n team khi khá»Ÿi Ä‘á»™ng
local function AutoSelectTeam()
    local team = getgenv().Team -- Biáº¿n lÆ°u team cáº§n chá»n, máº·c Ä‘á»‹nh lÃ  Pirate
    
    if team == "Pirate" then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetTeam", "Pirates")
    elseif team == "Marine" then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetTeam", "Marines")
    end
end

-- Gá»i hÃ m AutoSelectTeam Ä‘á»ƒ tá»± Ä‘á»™ng chá»n team khi khá»Ÿi Ä‘á»™ng game
AutoSelectTeam()

